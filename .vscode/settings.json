{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.format": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#1667aa",
    "activityBar.activeBorder": "#ed72b5",
    "activityBar.background": "#1667aa",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#ed72b5",
    "activityBarBadge.foreground": "#15202b",
    "sash.hoverBorder": "#1667aa",
    "statusBar.background": "#104c7d",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#1667aa",
    "statusBarItem.remoteBackground": "#104c7d",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#104c7d",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#104c7d99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#104c7d"
